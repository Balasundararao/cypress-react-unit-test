{
  "name": "cypress-react-unit-test",
  "version": "0.0.0-development",
  "description": "Unit test React components using Cypress",
  "main": "dist",
  "no-module-for-now": "lib",
  "types": "lib",
  "scripts": {
    "test": "cypress run",
    "percy:test": "percy exec -- cypress run",
    "build": "npm run transpile",
    "cy:open": "cypress open",
    "transpile": "tsc",
    "watch": "tsc -w",
    "pretest": "npm run lint && npm run transpile",
    "lint": "standard --verbose --fix *.js src cypress/integration",
    "semantic-release": "semantic-release",
    "format": "prettier --write 'src/*.jsx' 'cypress/**/*.js' 'cypress/**/*.jsx' 'lib/*.ts'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "files": [
    "dist",
    "lib"
  ],
  "keywords": [
    "react",
    "cypress",
    "cypress-io",
    "test",
    "testing"
  ],
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@cypress/webpack-preprocessor": "4.1.3",
    "debug": "4.1.1",
    "find-yarn-workspace-root": "1.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-transform-modules-commonjs": "7.7.5",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.0.0",
    "@material-ui/core": "4.9.5",
    "@material-ui/icons": "4.5.1",
    "@material-ui/lab": "4.0.0-alpha.39",
    "@percy/cypress": "1.0.9",
    "@testing-library/cypress": "5.0.2",
    "@types/node": "9.6.49",
    "@types/react": "16.9.16",
    "@types/react-dom": "16.9.4",
    "axios": "0.18.1",
    "babel-loader": "8.0.6",
    "babel-plugin-module-resolver": "4.0.0",
    "common-tags": "1.8.0",
    "css-loader": "2.1.1",
    "cypress": "4.3.0",
    "cypress-plugin-snapshots": "1.2.9",
    "github-post-release": "1.13.1",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "prettier": "1.19.1",
    "pretty": "2.0.0",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-google-maps": "9.4.5",
    "react-router": "6.0.0-alpha.1",
    "react-router-dom": "6.0.0-alpha.1",
    "semantic-release": "^17.0.4",
    "standard": "14.3.3",
    "style-loader": "0.23.1",
    "styled-components": "5.0.0",
    "svg-url-loader": "3.0.3",
    "typescript": "3.7.3",
    "webpack": "4.33.0",
    "webpack-cli": "3.3.9",
    "@emotion/core": "10.0.22"
  },
  "standard": {
    "globals": [
      "Cypress",
      "cy",
      "expect"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bahmutov/cypress-react-unit-test.git"
  },
  "release": {
    "branches": [
      "master",
      {
        "name": "feature/cypress-mount-mode",
        "prerelease": "cypress-mount-mode"
      }
    ],
    "analyzeCommits": "simple-commit-message",
    "generateNotes": "github-post-release"
  }
}
